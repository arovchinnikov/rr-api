{
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/src/",
            "Core\\": "core/src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-yaml": "*",
        "ext-pdo": "*",
        "spiral/roadrunner": "2.10.1",
        "nesbot/carbon": "2.59.1"
    },
    "require-dev": {
        "doctrine/migrations": "3.5.2",
        "phpunit/phpunit": "9.5.25",
        "squizlabs/php_codesniffer": "3.7.1",
        "symfony/var-dumper": "6.2.x-dev",
        "vimeo/psalm": "^4.29"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
